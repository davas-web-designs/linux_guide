xrandr --newmode "2560x1080_50.00"  188.75  2560 2712 2976 3392  1080 1083 1093 1114 -hsync +vsync
xrandr --addmode HDMI-1 "2560x1080_50.00"
xrandr --output LVDS-1 --primary --mode 1366x768 --pos 535x1080 --rotate normal --output VGA-1 --off --output HDMI-1 --mode 2560x1080_50.00 --pos 0x0 --rotate normal --output DP-1 --off --output HDMI-2 --off --output HDMI-3 --off --output DP-2 --off --output DP-3 --off
setbg ~/Downloads/bg.jpg
killall -q polybar
polybar example
